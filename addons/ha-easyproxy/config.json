  "slug": "easyproxy",
  "description": "Server Proxy Universale HLS con supporto FFmpeg",
  "arch": ["aarch64", "amd64", "armv7"],
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "options": {
    "global_proxy": "",
    "transport_routes": "",
